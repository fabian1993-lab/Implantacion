// Mostrar todos los registros de la tabla
// Forma 1
SELECT "Cod_prod", "Detalle_prod", "Cantidad_prod", "Precio_prod", "FechaIngreso_prod", 
       "Observaciones_prod"
  FROM "Productos";
  
// Forma 2
SELECT *
  FROM "Productos";

// Mostrar el promedio de los precios de los productos
SELECT AVG("Precio_prod") AS Promedio_Precios
  FROM "Productos";
  
 // Mostrar la suma de las cantidades de productos
SELECT SUM("Cantidad_prod") AS Cantidad_Productos
  FROM "Productos";

 // Mostrar el total de registros de la tabla productos
SELECT COUNT("Detalle_prod") AS Total_Registros
  FROM "Productos";
